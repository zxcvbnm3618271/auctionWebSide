<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	

	<bean id="auctionBIZ" class="com.qf.tesla.bizimpl.AuctionBIZImpl">
		<property name="auctionDao">
			<ref bean="auctionDao" />
		</property>
	</bean>

	<bean id="auctionRecordBIZ" class="com.qf.tesla.bizimpl.AuctionRecordBIZImpl">
		<property name="auctionRecordDao">
			<ref bean="auctionRecordDao" />
		</property>
	</bean>

	<bean id="auctionUserBIZ" class="com.qf.tesla.bizimpl.AuctionUserBIZImpl">
		<property name="auctionUserDao">
			<ref bean="auctionUserDao" />
		</property>
	</bean>


</beans>